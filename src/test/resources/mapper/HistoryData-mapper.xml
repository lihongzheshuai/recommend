<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.yunzhong.service.dao.HistoryDataDAO">

	<resultMap type="org.yunzhong.service.model.HistoryData" id="baseResultMap">
	</resultMap>


	<insert id="batchInsert" keyColumn="id" keyProperty="id"
		useGeneratedKeys="false">
		insert into history_datas(id, history_date,open, max,
		min,close,deal_count, deal_value)
		values
		<foreach collection="datas" item="data" separator=",">
			<trim prefix="(" suffix=")">
				#{data.id},
				#{data.date},
				#{data.open},
				#{data.max},
				#{data.min},
				#{data.close},
				#{data.dealCount},
				#{data.dealValue}
			</trim>
		</foreach>
	</insert>
</mapper>